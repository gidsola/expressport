{
  "name": "expressport",
  "private": true,
  "type": "module",
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev --workspace=client\" \"npm run dev --workspace=server\""
,
    "build": "npm run build --workspace=client",
    "start": "NODE_ENV=production node server/src/express.js"
  },
  "devDependencies": {
    "concurrently": "^9.2.1"
  }
}
